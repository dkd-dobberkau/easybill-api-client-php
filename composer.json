{
    "name": "dkd-dobberkau/easybill-api-client",
    "description": "PHP client for the Easybill REST API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dobberkau",
            "email": "olivier.dobberkau@dkd.de"
        }
    ],
    "keywords": ["easybill", "api", "client", "invoicing"],
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dkd\\EasybillClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dkd\\EasybillClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
